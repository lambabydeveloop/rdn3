    <script>
        function reveal() {
            const reveals = document.querySelectorAll('.reveal');
            for (let i = 0; i < reveals.length; i++) {
                const windowHeight = window.innerHeight;
                const elementTop = reveals[i].getBoundingClientRect().top;
                const elementVisible = 150;
                if (elementTop < windowHeight - elementVisible) {
                    reveals[i].classList.add('active');
                }
            }
        }

        window.addEventListener('scroll', reveal);
        window.addEventListener('load', reveal);

        function toggleFaq(button) {
            const answer = button.nextElementSibling;
            const icon = button.querySelector('svg');
            answer.classList.toggle('hidden');
            icon.classList.toggle('rotate-180');
        }

        function updateCalc() {
            const budget = document.getElementById('budget').value;
            const cpl = document.getElementById('cpl').value;
            const conversion = document.getElementById('conversion').value;

            document.getElementById('budgetValue').textContent = Number(budget).toLocaleString() + ' BYN';
            document.getElementById('cplValue').textContent = cpl + ' BYN';
            document.getElementById('conversionValue').textContent = conversion + '%';

            // –†–∞—Å—á–µ—Ç—ã –Ω–∞ –æ—Å–Ω–æ–≤–µ —Å—Ä–µ–¥–Ω–µ–≥–æ —Å–Ω–∏–∂–µ–Ω–∏—è CPL –Ω–∞ 42%
            const leadsNow = Math.round(budget / cpl);
            const cplAfter = Math.round(cpl * 0.58); // —Å–Ω–∏–∂–µ–Ω–∏–µ –Ω–∞ 42%
            const leadsAfter = Math.round(budget / cplAfter);
            const savings = budget - (leadsAfter * cpl);

            document.getElementById('leadsNow').textContent = leadsNow;
            document.getElementById('leadsAfter').textContent = leadsAfter;
            document.getElementById('cplNow').textContent = cpl + ' BYN';
            document.getElementById('cplAfter').textContent = cplAfter + ' BYN';
            document.getElementById('savings').textContent = Math.round(savings).toLocaleString() + ' BYN';

            // –ü—Ä–æ–≥—Ä–µ—Å—Å-–±–∞—Ä—ã
            const leadsPercent = Math.round((leadsAfter / (leadsNow * 2)) * 100);
            const cplPercent = Math.round((1 - (cplAfter / cpl)) * 100);

            document.getElementById('leadsProgress').style.width = Math.min(leadsPercent, 100) + '%';
            document.getElementById('cplProgress').style.width = cplPercent + '%';
        }

        document.querySelectorAll('a[href^="#"]').forEach(anchor => {
            anchor.addEventListener('click', function (e) {
                e.preventDefault();
                const target = document.querySelector(this.getAttribute('href'));
                if (target) {
                    target.scrollIntoView({ behavior: 'smooth', block: 'start' });
                }
            });
        });

        window.onload = updateCalc;
    </script>

    <style>
        .animation-delay-2000 { animation-delay: 2s; }
        .rotate-180 { transform: rotate(180deg); }
    </style>
